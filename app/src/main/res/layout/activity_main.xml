<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<RelativeLayout
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="#a3a691"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="64dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="174dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="173dp"
        android:drawableStart="@drawable/baseline_spoke_24" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="64dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="257dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="90dp"
        android:drawableStart="@drawable/baseline_spoke_24" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="64dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="102dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="245dp"
        android:drawableStart="@drawable/baseline_spoke_24" />

    <TextView
        android:id="@+id/register"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="100dp"

        android:layout_marginTop="81dp"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="40dp"
        android:text="Register"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="35dp" />

    <TextView

        android:id="@+id/textNL"
        android:layout_width="279dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/register"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="131dp"
        android:drawableLeft="@drawable/person"
        android:text="Masukkan Nama Lengkap"
        android:textColor="@color/white"
        android:textSize="20dp"

        />

    <EditText
        android:id="@+id/editNL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textNL"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="-4dp"
        android:layout_marginEnd="1dp"
        android:hint="Masukkan nama Lengkap"
        android:textColorHint="@color/white"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textUN"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editNL"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="174dp"
        android:drawableLeft="@drawable/person"
        android:text="Masukkan UserName"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/editUN"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textUN"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="-1dp"
        android:hint="Masukkan Username"
        android:textColorHint="@color/white"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textEmail"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editUN"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="170dp"
        android:drawableLeft="@drawable/email"
        android:text="Masukkan Email"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/editEmail"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textEmail"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="0dp"
        android:hint="Masukkan Email"
        android:textColorHint="@color/white"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textPassword"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editEmail"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="170dp"
        android:drawableLeft="@drawable/lock"
        android:text="Masukkan Password"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/editpassword"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textPassword"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="2dp"
        android:password="true"
        android:hint="Masukkan password"
        android:textColorHint="@color/white"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textKP"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editpassword"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="167dp"
        android:drawableLeft="@drawable/lock"
        android:text="Konfirmasi Password"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/editKP"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textKP"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="-2dp"
        android:hint="Masukkan password"
        android:password="true"
        android:textColorHint="@color/white"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textGender"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editKP"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="170dp"
        android:drawableLeft="@drawable/gender"
        android:text="Gender"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <Spinner
        android:id="@+id/gender"
        android:layout_width="407dp"
        android:layout_height="34dp"
        android:layout_below="@id/textGender"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="0dp"
        android:entries="@array/name_app"


        />
    <TextView
        android:id="@+id/textTL"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gender"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="170dp"
        android:drawableLeft="@drawable/gender"
        android:text="Tanggal Lahir"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <DatePicker
        android:id="@+id/tanggal"
        android:layout_width="match_parent"
        android:layout_height="124dp"
        android:layout_below="@+id/textTL"
        android:layout_marginTop="3dp"
        android:calendarViewShown="false"
        android:datePickerMode="spinner"
        android:calendarTextColor="@color/black"
        />


    <TextView
        android:id="@+id/texthp"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tanggal"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="170dp"
        android:drawableLeft="@drawable/phone"
        android:text="No Hp"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/edithp"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/texthp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="-1dp"
        android:hint="Masukkan No Hp"
        android:textColorHint="@color/white"
        android:textSize="15dp" />


    <TextView
        android:id="@+id/textalamat"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/edithp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="170dp"
        android:drawableLeft="@drawable/home"
        android:text="Alamat"
        android:textColor="@color/white"
        android:textSize="20dp" />
    <EditText
        android:id="@+id/editalamat"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textalamat"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="-1dp"
        android:hint="Masukkan Alamat"
        android:textColorHint="@color/white"
        android:textSize="15dp" />

    <Button
        android:background="@color/black"

        android:id="@+id/button"
        android:layout_width="350dp"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editalamat"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:text="masuk" />


</RelativeLayout>

</ScrollView>